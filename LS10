#include <iostream>
using namespace std;

int sumElements(int arr[], int n) {
    int sum = 0;
    for (int i = 0; i < n; i++) {
        sum += arr[i]; // Add each element to sum
    }
    return sum;
}

int main() {
    int arr[] = {2, 3, 5, 7};
    int n = sizeof(arr) / sizeof(arr[0]);
    
    int result = sumElements(arr, n);
    cout << "Sum of all elements: " << result << endl;
    
    return 0;
}
